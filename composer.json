{
  "name": "epfremmer/swagger-bundle",
  "type": "symfony-bundle",
  "description": "Symfony 2 bundle for leveraging swagger library to generate application code",
  "license": "MIT",
  "authors": [
    {
      "name": "Edward Pfremmer",
      "email": "epfremme@nerdery.com"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:epfremmer/swagger.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "ERP\\SwaggerBundle\\": "",
      "Test\\TestBundle\\": "src/Test/TestBundle"
    }
  },
  "require": {
    "php": ">=5.5",
    "epfremmer/swagger": "~0.1",
    "symfony/symfony": "~2.7"
  },
  "require-dev": {
    "behat/behat": "~3.0",
    "behat/symfony2-extension": "~2.0",
    "behat/mink": "~1.6",
    "behat/mink-extension": "~2.0",
    "behat/mink-browserkit-driver": "~1.2",
    "sensio/distribution-bundle": "~4.0",
    "sensio/generator-bundle": "^2.5",
    "sensio/framework-extra-bundle": "^3.0"
  },
  "scripts": {
    "post-root-package-install": [
      "SymfonyStandard\\Composer::hookRootPackageInstall"
    ],
    "post-install-cmd": [
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
    ],
    "post-update-cmd": [
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
    ]
  }
}
